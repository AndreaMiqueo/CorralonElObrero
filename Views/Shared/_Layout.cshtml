
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CoralónElObrero</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/Estilos.css" />
    <script src="https://use.fontawesome.com/93decae897.js"></script>
    <script src="~/js/JavaScript.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.1/jquery.validate.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                    <img style="height:90px" src="~/imag/logo.png"/>
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-end " style="font-size:20px">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a  class="nav-link text-dark active" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="QueOfrecemos">Qué ofrecemos</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="CorralónDeMateriales" >Corralón de materiales</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Contacto">Contacto</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="contenedor">
        <main role="main" class="pb-3">
            @RenderBody()
            <a class="ir-arriba" style="display:inline-block" href="#">
                <i class="fa fa-chevron-circle-up fa-3x" style="color:black" aria-hidden="true" ></i>
            </a>
        </main>
        


    </div>
    <footer class="border-top contFooter text-muted">
        <div class="contenidFooter">
            &copy; 2021 - CORRALÓN Y PINTURERIA EL OBRERO.
            <p>
                <strong>
                    Todos los derechos reservados.
                </strong>
            </p>
            <ul style="padding-inline-start:0px;">
                <li>
                    <strong >
                        <a class="politicas" asp-area="" asp-controller="Home" asp-action="Legales">Aviso legal</a>
                    </strong>
                </li>
                <li>
                    <strong >
                        <a class="politicas" asp-area="" asp-controller="Home" asp-action="Privacy">Politica de Privacida</a>
                    </strong>
                </li>
                <li>
                    <strong>
                        <a class="politicas" asp-area="" asp-controller="Home" asp-action="MapaWeb">Mapa Web</a>
                    </strong>
                </li>
            </ul>
        </div>
        
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false);
</body>
</html>
